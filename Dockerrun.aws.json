{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "client",
        "image": "engsasa/multi-client",
        "hostname": "client",
        "essential": false
      },
      {
        "name": "server",
        "image": "engsasa/multi-server",
        "hostname": "api",
        "essential": false
      },
      {
        "name": "worker",
        "image": "engsasa/multi-worker",
        "hostname": "worker",
        "essential": false
      },
      {
        "name": "nginx",
        "image": "engsasa/multi-nginx",
        "hostname": "nginx",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPoort": 80
          }
        ]
      }
    ]
  }